<p id="notice"><%= notice %></p>

<h1>Redirect Logs</h1>

<table>
  <thead>
    <tr>
      <th>Old url</th>
      <th>New url</th>
      <th>Rule</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @redirect_logs.each do |redirect_log| %>
      <tr>
        <td><%= redirect_log.old_url %></td>
        <td><%= redirect_log.new_url %></td>
        <td><%= redirect_log.rule %></td>
        <td><%= redirect_log.status %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
