<%= form_with(model: redirect_log, local: true) do |form| %>
  <% if redirect_log.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(redirect_log.errors.count, "error") %> prohibited this redirect_log from being saved:</h2>

      <ul>
      <% redirect_log.errors.full_messages.each do |message| %>
